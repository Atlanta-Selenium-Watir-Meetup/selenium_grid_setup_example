{
    "capabilities": [
        {
            "browserName": "IPAD",
            "version": "6.1",
            "device": "IPAD Simulator",
            "maxInstances": 1,
            "platform": "MAC",
            "app": "safari"
        }
    ],
    "configuration": {
        "cleanUpCycle": 2000,
        "timeout": 30000,
        "proxy": "org.openqa.grid.selenium.proxy.DefaultRemoteProxy",
        "url": "http://<machine ip address>:4723/wd/hub",
        "maxSession": 1,
        "port": 4723,
        "host": "<machine ip address>",
        "register": true,
        "registerCycle": 5000,
        "hubPort": 9000,
        "hubHost": "<grid url>"
    }
}
